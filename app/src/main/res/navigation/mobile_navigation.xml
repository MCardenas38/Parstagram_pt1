<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.mcardenas.parstagram.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_postPic"
        android:name="com.mcardenas.parstagram.ui.post.PostFragment"
        android:label="@string/title_Post"
        tools:layout="@layout/fragment_post" />

    <fragment
        android:id="@+id/navigation_logOut"
        android:name="com.mcardenas.parstagram.ui.signout.SignOutFragment"
        android:label="@string/title_Options"
        tools:layout="@layout/fragment_signout" />
</navigation>